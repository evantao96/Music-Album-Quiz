<h4 id="log_in">Log In</h4>

<p id="notice"><%= notice %></p>

<font size="1"><%= form_tag url: create_login_path do %>
  <%= label_tag :name %><br>
  <%= text_field_tag :name %>
  <br>
  <%= label_tag :password %><br>
  <%= password_field_tag :password %>
  <br>
  <%= submit_tag 'Log in' %>
<% end %></font>
